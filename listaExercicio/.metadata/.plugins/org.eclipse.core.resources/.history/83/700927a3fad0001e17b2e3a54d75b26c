<?php

class Endereco {
    
  private $logradouro;  
  private $numero;
  private $cidade;
  
  public function __construct($a, $b, $c){
      
      $this->logradouro = $a;
      $this->numero = $b;
      $this->cidade = $c;
      
  }
  
  public function __destruct(){
      
      var_dump("Destruir");
      
  }
    
}


$meuEndereco = new Endereco("Rua Travessa SÃ£o Nicolau", "25", "Fortaleza");

var_dump($meuEndereco);

unset($meuEndereco);

?>


